    <ownedComment xmi:type="uml:Comment" xmi:id="_aSS3wEJtEeSQQvkAoKcNfQ">
      <annotatedElement xmi:type="uml:Class" href="../requirements/OpenES_Requirements.uml#_Lc0NgDg2EeSL_oCrEVX-XQ"/>
      <annotatedElement xmi:type="uml:Profile" href="../../OSMK/osmk.profile.uml#_U-5z8D8qEeSQQvkAoKcNfQ"/>
